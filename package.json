{
  "name": "tt-search",
  "version": "1.0.0",
  "description": "Professional TikTok search, analytics & data extraction with Parquet storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tt-search": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && npm run build:python",
    "build:python": "python -m py_compile python/**/*.py",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build",
    "setup:python": "pip install -r requirements.txt",
    "cli": "node dist/cli/index.js"
  },
  "keywords": [
    "tiktok",
    "search",
    "scraping",
    "analytics",
    "parquet",
    "data-extraction",
    "social-media",
    "api",
    "trending",
    "hashtags"
  ],
  "author": "Brashkie",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brashkie/tt-search.git"
  },
  "bugs": {
    "url": "https://github.com/brashkie/tt-search/issues"
  },
  "homepage": "https://github.com/brashkie/tt-search#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "p-queue": "^8.0.1",
    "p-retry": "^6.1.0",
    "date-fns": "^3.0.6",
    "zod": "^3.22.4",
    "cache-manager": "^5.3.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "python",
    "requirements.txt",
    "README.md",
    "LICENSE"
  ]
}
